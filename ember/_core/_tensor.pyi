from __future__ import annotations
from typing import List, Tuple, Union

class _Tensor:
    def __init__(self, size: int) -> None: ...
    @property
    def size(self) -> int: ...
    def copy_from_list(self, data: List[float]) -> None: ...
    def to_list(
        self, shape: Tuple[int, ...]
    ) -> Union[List[float], List[List[float]]]: ...

def _add(a: _Tensor, b: _Tensor) -> _Tensor: ...
def _subtract(a: _Tensor, b: _Tensor) -> _Tensor: ...
def _multiply_elementwise(a: _Tensor, b: _Tensor) -> _Tensor: ...
def _negate(tensor: _Tensor) -> _Tensor: ...
def _matmul(a: _Tensor, b: _Tensor, n: int, m: int, k: int) -> _Tensor: ...
